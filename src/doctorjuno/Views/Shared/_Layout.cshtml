<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <meta name="description" content="doctorjuno makes websites and more" />
    <meta name="robots" content="index,follow" />
    <meta name="googlebot" content="index,follow" />
    <title>doctorjuno</title>

    <environment names="Development">
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons"
              rel="stylesheet">
        <link rel="stylesheet" href="~/css/materialize.css" />
        <link rel="stylesheet" href="~/sass/site.css" />
    </environment>
    <environment names="Staging,Production">
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/css/materialize.min.css"
              asp-fallback-href="~/css/materialize.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="~/sass/site.min.css" asp-append-version="true" />
    </environment>
</head>
<body>
    <div id="near_bg"></div>
    <div id="mid_bg"></div>
    <div class="container-fluid">
        @Html.Partial("navbar")
        @RenderBody()
    </div>
    <environment names="Development">
        <script src="~/lib/materialize.es5.js" asp-append-version="false"></script>
        <script src="~/lib/jquery.es5.js" asp-append-version="false"></script>

        <script src="~/js/site.js" asp-append-version="false"></script>
    </environment>
    <environment names="Staging,Production">

        <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/js/materialize.min.js"
                asp-fallback-src="~/lib/materialize.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal">
        </script>
        <script src="~/js/site.es5.min.js" asp-append-version="true"></script>
    </environment>
    @RenderSection("scripts", required: false)
</body>
</html>